{
  "name": "asheronsatlas",
  "version": "0.1.0-alpha",
  "main": "dist-electron/main/index.js",
  "description": "Asheron's Atlas development repository.",
  "productName": "Asheron's Atlas",
  "author": "AsheronDB <hello@asherondb.com>",
  "license": "MIT",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/AsheronDB/AsheronsAtlas"
  },
  "scripts": {
    "dev": "vite",
    "preview": "vite preview",
    "build": "vite build && electron-builder -mw",
    "build:mac": "vite build && electron-builder -m",
    "build:win": "vite build && electron-builder -w",
    "publish:mac": "vite build && electron-builder -m --publish=always",
    "publish:win": "vite build && electron-builder -w --publish=always"
  },
  "build": {
    "productName": "Asheron's Atlas",
    "extraMetadata": {
      "name": "Asherons Atlas"
    },
    "appId": "com.asherondb.asheronsatlas",
    "copyright": "Copyright Â© 2023 AsheronDB",
    "generateUpdatesFilesForAllChannels": true,
    "asar": true,
    "icon": "public/favicon.ico",
    "directories": {
      "output": "release/${version}"
    },
    "files": [
      "dist-electron",
      "dist"
    ],
    "mac": {
      "artifactName": "AsheronsAtlas_${version}.${ext}",
      "executableName": "Asheron's Atlas",
      "category": "public.app-category.reference",
      "hardenedRuntime": true,
      "publish": [
        {
          "provider": "github"
        }
      ],
      "target": [
        {
          "target": "dmg",
          "arch": [
            "universal"
          ]
        }
      ]
    },
    "win": {
      "artifactName": "AsheronsAtlas_${version}.${ext}",
      "executableName": "Asheron's Atlas",
      "verifyUpdateCodeSignature": false,
      "publish": [
        "github"
      ],
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ]
    },
    "nsis": {
      "oneClick": false,
      "perMachine": true,
      "allowToChangeInstallationDirectory": true,
      "deleteAppDataOnUninstall": true
    }
  },
  "debug": {
    "env": {
      "VITE_DEV_SERVER_URL": "http://127.0.0.1:3344/"
    }
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.0.0",
    "autoprefixer": "^10.4.13",
    "electron": "^23.1.0",
    "electron-builder": "^23.6.0",
    "postcss": "^8.4.21",
    "tailwindcss": "^3.2.7",
    "typescript": "^4.9.5",
    "vite": "^4.1.3",
    "vite-plugin-electron": "^0.11.1",
    "vite-plugin-electron-renderer": "^0.12.1",
    "vite-svg-loader": "^4.0.0",
    "vue": "^3.2.47",
    "vue-tsc": "^1.1.5"
  },
  "dependencies": {
    "@asherondb/ac-position": "^1.0.2",
    "@fortawesome/fontawesome-svg-core": "^6.3.0",
    "@fortawesome/free-brands-svg-icons": "^6.3.0",
    "@fortawesome/free-regular-svg-icons": "^6.3.0",
    "@fortawesome/free-solid-svg-icons": "^6.3.0",
    "@fortawesome/vue-fontawesome": "^3.0.3",
    "@headlessui/vue": "^1.7.10",
    "@turf/turf": "^6.5.0",
    "body-parser": "^1.20.1",
    "cors": "^2.8.5",
    "date-fns": "^2.29.3",
    "electron-json-storage": "^4.6.0",
    "electron-log": "^5.0.0-beta.19",
    "electron-updater": "^5.3.0",
    "express": "^4.18.2",
    "flexsearch": "^0.7.31",
    "flowbite": "^1.6.3",
    "flowbite-vue": "^0.0.10",
    "leaflet": "^1.9.3",
    "lodash": "^4.17.21",
    "pinia": "^2.0.32",
    "slugify": "^1.6.5",
    "vue-router": "^4.1.6"
  }
}
